<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/navavava_gaaama"
    app:startDestination="@id/enterotoFragment">

    <fragment
        android:id="@+id/enterotoFragment"
        android:name="com.skgames.trafficriders.frrragagag.EnterotoFragment"
        android:label="fragment_enteroto"
        tools:layout="@layout/fragment_enteroto" >
        <action
            android:id="@+id/action_enterotoFragment_to_ruuulesFragment"
            app:destination="@id/ruuulesFragment" />
    </fragment>
    <fragment
        android:id="@+id/ruuulesFragment"
        android:name="com.skgames.trafficriders.frrrag22.RuuulesFragment"
        android:label="fragment_ruuules"
        tools:layout="@layout/fragment_ruuules" >
        <action
            android:id="@+id/action_ruuulesFragment_to_choooseDiffFragment"
