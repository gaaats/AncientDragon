        tools:layout="@layout/fragment_plaaay_gaaame" >
        <action
            android:id="@+id/action_plaaayGaaameFragment_to_restartttttttFragment"
            app:destination="@id/restartttttttFragment" />
    </fragment>
    <fragment
        android:id="@+id/restartttttttFragment"
        android:name="com.skgames.trafficriders.RestartttttttFragment"
        android:label="fragment_restarttttttt"
        tools:layout="@layout/fragment_restarttttttt" >
        <action
            android:id="@+id/action_restartttttttFragment_to_choooseDiffFragment"
            app:destination="@id/choooseDiffFragment" />
    </fragment>
</navigation>